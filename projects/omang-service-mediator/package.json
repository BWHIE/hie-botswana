    "@nestjs/platform-express": "^7.6.18",
    "@nestjs/schedule": "^2.0.4",
    "@nestjs/typeorm": "^7.1.5",
    "@smile-cdr/fhirts": "^2.1.1",
    "axios": "^1.6.8",
    "cookie-parser": "^1.4.6",
    "g": "^2.0.1",
    "nconf": "^0.12.1",
